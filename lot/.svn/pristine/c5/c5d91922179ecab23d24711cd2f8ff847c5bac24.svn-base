@import "./function.scss";

.or {
  margin-top: 20px;
  .box {
    width: 912px;
    // margin: 0 auto;
    // padding-top: 7px;

    padding-left: 20px;
    padding-right: 12px;
    overflow: hidden;
    .uls {
      width: 20%;
      float: left;
      border-left: 1px solid $border_color;
      .thend {
        height: 40px;
        line-height: 40px;
        background-color: #eee;
        border-top: 1px solid $border_color;
        border-bottom: 1px solid $border_color;
      }
      .list {
        border-bottom: 1px solid $border_color;
      }
      .list:last-child {
        border-bottom: none;
      }
      // overflow: hidden;
      .one, .tow {
        float: left;
        // display: inline-block;
        width: 50px;
        height: 40px;
        line-height: 40px;
        color: #000;
        font-weight: 700;
        cursor: pointer;
      }
      .tow {
        border-left: 1px solid $border_color;
        border-right: 1px solid $border_color;
      }
      .three {
        float: left;
        line-height: 40px;
        height: 40px;
        width: 74px;
        input {
          float: left;
          width: 36px;
          height: 16px;
          color: #000;
          border: 1px solid #b1b1b1;
        }
      }
    }
  }
  .uls:nth-child(5), .uls:last-child {
    border-right: 1px solid $border_color;
  }
  .uls:nth-child(6), .uls:last-child {
    width: 50%;
    height: 204px;
    border-bottom: 1px solid $border_color;
    .one, .tow {
      width: 98px;
    }
    .three {
      width: 235px;
    }
  }
  .uls:last-child {
    .list {
      border-bottom: 1px solid $border_color !important;
    }
  }
}

.footer1 {
  width: 920px;
  float: left;
  margin-top: 10px;
  padding-right: 20px;
  // padding-bottom: 45px;
  .input {
    width: 85px;
    padding: 5px;
    background-color: #eeeeee;
    margin-right: 10px;
    border-radius: 5px;
    float: right;
  }
  button {
    width: 85px;
    padding: 5px;
    float: right;
    outline: none 0;
    border-radius: 5px;
    margin-top: 12px;
    cursor: pointer;
  }
  .img {
    margin-left: 136px;
    float: left;
    span {
      //margin-left: 30px;
      //height: 60px;
      float: left;
      display: block;
      cursor: pointer;
    }
    .s1 {
      height: 60px;
      width: 64px;
      background: url("../../assets/img/Chips.png") no-repeat 0 0px;
      background-size: 368px;
    }
    .s2 {
      height: 60px;
      width: 64px;
      background: url("../../assets/img/Chips.png") no-repeat -64px 0px;
      background-size: 368px;
    }
    .s3 {
      height: 60px;
      width: 64px;
      background: url("../../assets/img/Chips.png") no-repeat -128px 0px;
      background-size: 368px;
    }
    .s4 {
      height: 60px;
      width: 64px;
      background: url("../../assets/img/Chips.png") no-repeat -192px 0px;
      background-size: 368px;
    }
    .s5 {
      height: 60px;
      width: 64px;
      background: url("../../assets/img/Chips.png") no-repeat -256px 0px;
      background-size: 368px;
    }
    .s6 {
      height: 60px;
      width: 73px;
      background: url("../../assets/img/Chips.png") no-repeat -320px 0px;
      background-size: 368px;
    }
  }
  .one {
    width: 160px;
    float: left;
    background-color: #eee;
    padding: 5px;
    border-radius: 10px;
    margin-top: 12px;
    border: none;
    span {
      color: $bg_color;
    }
    .left {
      color: #999999
    }
  }
  .two {
    float: right;
    background-color: $bg_color;
    color: #fff
  }
}

.styleclect {
  background-color: $bg_select;
}
