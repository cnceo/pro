@import "./function.scss";

.bj_tooface {
  .bj_too_box {
    .ul1 {
      width: 880px;
      overflow: hidden;
      margin-left: 20px;
      border: 1px solid $border_color;
      .thend {
        display: inline-block;
        width: 880px;
        background-color: #eee;
        height: 30px;
        line-height: 30px;
        border-bottom: 1px solid $border_color;

      }
      li {
        width: 219px;
        float: left;
        border-right: 1px solid $border_color;
        .one, .tow {
          display: block;
          float: left;
          width: 70px;
          height: 40px;
          line-height: 40px;
          border-right: 1px solid $border_color;
          cursor: pointer;
        }
        .three {
          height: 40px;
          width: 70px;
          line-height: 40px;
          display: inline-block;
          input {
            width: 42px;
            height: 16px;
            color: #000;
            border: 1px solid #b1b1b1;
          }
        }
      }
      li:last-child {
        border-right: none;
      }
    }
    .box2 {
      margin-left: 20px;
      overflow: hidden;
      .ul2 {
        float: left;
        overflow: hidden;
        border-right: 1px solid $border_color;
        width: 176px;
        .thend2 {
          border-bottom: 1px solid $border_color;
          width: 176px;
          display: inline-block;
          height: 30px;
          line-height: 30px;
          background-color: #eee;
        }
        li {
          border-bottom: 1px solid $border_color;
          .one2, .tow2 {
            display: block;
            float: left;
            width: 56px;
            line-height: 40px;
            height: 40px;
            border-right: 1px solid $border_color;
            cursor: pointer;
          }
          .three2 {
            display: inline-block;
            line-height: 40px;
            height: 40px;
            width: 56px;
            input {
              width: 36px;
              height: 16px;
              color: #000;
              border: 1px solid #b1b1b1;
            }
          }
        }
      }
      .ul2:first-child {
        border-left: 1px solid $border_color;
      }
      .ul3 {
        float: left;
        overflow: hidden;
        border-right: 1px solid $border_color;
        width: 176px;
        .thend2 {
          border-bottom: 1px solid $border_color;
          width: 176px;
          display: inline-block;
          height: 30px;
          line-height: 30px;
          background-color: #eee;
        }
        li {
          border-bottom: 1px solid $border_color;
          .one2, .tow2 {
            display: block;
            float: left;
            width: 56px;
            line-height: 40px;
            height: 40px;
            border-right: 1px solid $border_color;
            cursor: pointer;
          }
          .three2 {
            display: inline-block;
            line-height: 40px;
            height: 40px;
            width: 56px;
            input {
              width: 36px;
              height: 16px;
              color: #000;
              border: 1px solid #b1b1b1;
            }
          }
        }
      }
      .ul3:first-child {
        border-left: 1px solid $border_color;
      }
    }
  }
  .footer1 {
    width: 920px;
    float: left;
    margin-top: 10px;
    padding-right: 20px;
    // padding-bottom: 45px;
    .input {
      width: 85px;
      padding: 5px;
      background-color: #eeeeee;
      margin-right: 10px;
      border-radius: 5px;
      float: right;
    }
    button {
      width: 85px;
      padding: 5px;
      float: right;
      outline: none 0;
      border-radius: 5px;
      margin-top: 12px;
      cursor: pointer;
    }
    .img {
      margin-left: 136px;
      float: left;
      span {
        //margin-left: 30px;
        //height: 60px;
        float: left;
        display: block;
        cursor: pointer;
      }
      .s1 {
        height: 60px;
        width: 64px;
        background: url("../../assets/img/Chips.png") no-repeat 0 0px;
        background-size: 368px;
      }
      .s2 {
        height: 60px;
        width: 64px;
        background: url("../../assets/img/Chips.png") no-repeat -64px 0px;
        background-size: 368px;
      }
      .s3 {
        height: 60px;
        width: 64px;
        background: url("../../assets/img/Chips.png") no-repeat -128px 0px;
        background-size: 368px;
      }
      .s4 {
        height: 60px;
        width: 64px;
        background: url("../../assets/img/Chips.png") no-repeat -192px 0px;
        background-size: 368px;
      }
      .s5 {
        height: 60px;
        width: 64px;
        background: url("../../assets/img/Chips.png") no-repeat -256px 0px;
        background-size: 368px;
      }
      .s6 {
        height: 60px;
        width: 73px;
        background: url("../../assets/img/Chips.png") no-repeat -320px 0px;
        background-size: 368px;
      }
    }
    .one {
      width: 160px;
      float: left;
      background-color: #eee;
      //margin-left: 105px;
      span {
        color: $bg_color;
      }
      .left {
        color: #999999
      }
    }
    .two {
      float: right;
      background-color: $bg_color;
      color: #fff
    }
  }
}

.styleclect {
  background-color: $bg_select;
}
