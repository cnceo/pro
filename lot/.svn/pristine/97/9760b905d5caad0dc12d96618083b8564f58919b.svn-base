           //pjax局部加载列表
    function renderTableByPjax() {
       
        var ptype = $.trim($('#ptype').val());
        var starttime = $.trim($('#starttime').val());
        var endtime = $.trim($('#endtime').val());
        var uname = $.trim($('#uname').val());
        var pageNum = $.trim($('#pageNum').val());
        var page = $.trim($('#page').val());
        var params = {
            ptype: ptype,
            starttime: starttime,
            endtime: endtime,
            uname: uname,
            pageNum: pageNum,
            page: page
        };
        $.pjax({
            data: params,
            method: 'get',
            url: '/log/history',
            container: '#container'
        });
    }
    //搜索菜单
    $('#search').click(function(){
        renderTableByPjax();
    })
    //页码
    $('#page').change(function(){
       renderTableByPjax();
    })

      //时间控件
    var start = {
        elem: '#starttime',
        format: 'YYYY-MM-DD',
        max: laydate.now(),
        istime: false,
        istoday: true,
        choose: function (datas) {
            end.min = datas;
        }
    };
    laydate(start);
    var end = {
