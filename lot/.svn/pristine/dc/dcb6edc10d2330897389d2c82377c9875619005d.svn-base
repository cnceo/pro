@import "./function.scss";

.game_rule {
  p {
    text-align: left;
  }
  .pk_bet {
    position: fixed;
    left: 0px;
    top: 0px;
    z-index: 100;
    background: gray;
    opacity: 0.8;
    width: 100%;
    height: 100%;
  }
  .demo-tabs-style2 {
    width: 1000px;
    margin: auto;
    padding-top: 50px;
  }
  .g_r_box {
    overflow: hidden;
    background-color: #fff;
    position: fixed;
    z-index: 100;
    width: 1050px;
    height: 600px;
    top: 5%;
    left: 15%;
    //margin-left: -520px;
    border-radius: 8px 8px;
    .top {
      z-index: 100;
      overflow: hidden;
      width: 1050px;
      position: relative;
      border-radius: 5px 5px;
      height: 50px;
      background: $bg_color3;
      span {
        line-height: 50px;
        font-size: 18px;
        color: #fff;
        font-weight: bold;
      }
      .rule {
        margin-left: 20px;
      }
      .close {
        float: right;
        margin-right: 20px;
      }
    }

    .nav {
      padding: 0 30px;
      overflow: hidden;
      margin-left: 22px;
      margin-right: 22px;
      float: left;
      position: relative;
      width: 1000px;
      height: 60px;
      line-height: 60px;
      .i1 {
        left: 0;
        top: 0px;
        position: absolute;
        font-size: 24px;
      }
      .i2 {
        right: 0;
        top: 0px;
        position: absolute;
        font-size: 24px;

      }
      ul {
        height: 60px;
        width: 3000px;
        position: absolute;
        li {
          display: inline-block;
          margin: 10px 9px;
          height: 40px;
          line-height: 40px;
          font-size: 16px;
          float: left;
          width: 100px;
          span {
            display: inline-block;
            //margin: 10px 9px;
            height: 40px;
            line-height: 40px;
            font-size: 16px;
            float: left;
            width: 100px;
            color: #666;
          }
          .styleclect {
            background-color: #028BE1;
            width: 100px;
            border-radius: 8px;
            color: #fff;
          }
        }
      }
    }
