      width: 860px;
      padding: 20px;
      border-radius: 5px;
      p {
        font-size: 14px;
        text-align-last: left;
      }
      h2 {
        margin-top: 10px;
        color: red;
        text-align-last: left;
      }
      .box {
        margin-bottom: 100px;
      }
      .box1 {
        h2 {
