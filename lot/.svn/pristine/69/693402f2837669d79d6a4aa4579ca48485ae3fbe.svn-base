@import "./function.scss";

.six_one {
  width: 920px;
  overflow: hidden;
  //padding-top: 17px;
  .content {
    // width: 885px;
    overflow: hidden;
    float: left;
    margin-left: 20px;
    margin-bottom: 10px;
    .top {
      border: 1px solid $border_color;
      ul {
        overflow: hidden;
        background-color: #eeeeee;
        color: #999999;
        li {
          float: left;
          padding: 10px 0;
        }
        li:last-child {
          border-right: none 0;
        }
        .only {
          width: 55px;
          border-right: 1px solid $border_color
        }
        .one {
          width: 137px;
          border-right: 1px solid $border_color
        }
        // .two{
        //   width: 60px
        // }
        // .three{
        //   width: 60px;
        //
        // }
      }
    }
    .table {
      overflow: hidden;
      .bottom {
        float: left;
        ul {
          overflow: hidden;
          li {
            float: left;
            height: 39px;
            padding: 10px 0;
            border-bottom: 1px solid $border_color;
            border-left: 1px solid $border_color;
            cursor: pointer;
          }
          .border2_none {
            width: 60px;
            border-left: none 0 !important;
          }
          .border3_none {
            width: 62px;
            padding: 9px 0;
            border-left: none 0 !important;
            border-right: 1px solid $border_color;
          }
          .bg_color {
            background-color: $bg_select;
          }
          .one {
            width: 55px;
          }
          .two {
            width: 60px;
          }
          .two1 {
            width: 70px;
          }
          .three1 {
            width: 67px;
            padding: 9px 0;
            input {
              width: 40px;
              border: 1px solid #ddd
            }
          }
          .three {
            width: 60px;
            padding: 9px 0;
            input {
              width: 40px;
              border: 1px solid #ddd
            }
          }
        }
      }
      // .bottom:nth-child(3) ul:nth-child(11)>li:nth-child(3){
      //   border-right: 1px solid #ddd;
      // }
      .bottom:last-child ul > li.three1 {
        width: 69px;
        border-right: 1px solid $border_color;
      }
    }

  }
