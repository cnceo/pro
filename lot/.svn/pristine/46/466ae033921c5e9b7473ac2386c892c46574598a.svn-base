@import "./function.scss";

.nine {
  width: 920px;
  overflow: hidden;
  .content {
    width: 702px;
    overflow: hidden;
    float: left;
    margin-left: 20px;
    .top {
      border: 1px solid $border_color;
      ul {
        overflow: hidden;
        background-color: #eeeeee;
        color: #999999;
        li {
          float: left;
          padding: 10px 0;
        }
        li:last-child {
          border-right: none 0;
        }
        .one {
          width: 55px
        }
        .two {
          width: 60px
        }
        .three {
          width: 60px;
          border-right: 1px solid #999999
        }
      }
    }
    .table {
      overflow: hidden;
      .bottom {
        float: left;
        ul {
          overflow: hidden;
          li {
            height: 39px;
            float: left;
            padding: 10px 0;
            border-bottom: 1px solid $border_color;
            border-left: 1px solid $border_color;
            cursor: pointer;
          }
          .border2_none {
            width: 60px;
            border-left: none 0 !important;
          }
          .border3_none {
            width: 62px;
            padding: 9px 0;
            border-left: none 0 !important;
            border-right: 1px solid $border_color;
          }
          .one {
            width: 55px;
          }
          .two {
            width: 60px;
          }
          .three {
            width: 60px;
            padding: 9px 0;
            input {
              width: 40px;
              border: 1px solid #ddd
            }
          }
        }
      }
      .bottom:last-child ul > li.three {
        width: 62px;
        border-right: 1px solid $border_color;
      }
    }

  }
  .bottom_content {
    width: 877px;
    overflow: hidden;
    float: left;
    margin-left: 20px;
    // margin-top: 15px;
    .top {
      border: 1px solid $border_color;
      ul {
        overflow: hidden;
        background-color: #eeeeee;
        color: #999999;
        li {
          float: left;
          padding: 10px 0;
        }
        li:last-child {
          border-right: none 0;
        }
        .one {
          width: 55px
        }
        .two {
          width: 60px
        }
        .three {
          width: 60px;
        }
        .four {
          width: 260px;
          border-right: 1px solid #999999
        }
      }
    }
    .table {
      overflow: hidden;
      .bottom {
        float: left;
        ul {
          overflow: hidden;
          li {
            height: 39px;
            float: left;
            padding: 10px 0;
            border-bottom: 1px solid $border_color;
            border-left: 1px solid $border_color;
          }
          .border2_none {
            width: 60px;
            border-left: none 0 !important;
          }
          .border3_none {
            width: 62px;
            padding: 9px 0;
            border-left: none 0 !important;
            border-right: 1px solid $border_color;
          }
          .one {
            width: 55px;
            cursor: pointer;
          }
          .two {
            width: 61px;
            cursor: pointer;
          }
          .three {
            width: 60px;
            padding: 9px 0;
            input {
              width: 40px;
              border: 1px solid #ddd
            }
          }
          .four {
            width: 259px;
            padding: 0;
            span {
              width: 39px;
              height: 39px;
              line-height: 39px;
              background: url('../img/red_box.png') no-repeat;
              display: inline-block;
              color: #fff;
              font-weight: bold;
              font-size: 16px;
              background-size: cover;
            }
          }
        }
      }
      .bottom:last-child ul > li.four {
        width: 266px;
        border-right: 1px solid $border_color;
      }
    }

  }
  .footer1 {
    width: 920px;
    float: left;
    margin-top: 20px;
    padding-right: 20px;
    // padding-bottom: 45px;
    .input {
      width: 85px;
      padding: 5px;
      background-color: #eeeeee;
      margin-right: 10px;
      border-radius: 5px;
      float: right;
    }
    button {
      width: 85px;
      padding: 5px;
      float: right;
      outline: none 0;
    }
    .one {
      background-color: #eeeeee;
      margin-right: 15px;
      span {
        color: $bg_color
      }
      .left {
        color: #999999
      }
    }
    .two {
      background-color: $bg_color;
      color: #fff
    }
  }
  .footer2 {
    width: 920px;
    float: left;
    margin-top: 20px;
    padding-right: 20px;
    padding-bottom: 45px;
    .input {
      width: 85px;
      padding: 5px;
      background-color: #eeeeee;
      margin-right: 15px;
      border-radius: 5px;
      float: right;
    }
    button {
      width: 85px;
      padding: 5px;
      float: right;
      outline: none 0;
    }
    .one {
      background-color: #eeeeee;
      // margin-right: 15px;
      span {
        color: $bg_color
      }
      .left {
        color: #999999
      }
    }
    .two {
      background-color: $bg_color;
      color: #fff
    }
  }
}
