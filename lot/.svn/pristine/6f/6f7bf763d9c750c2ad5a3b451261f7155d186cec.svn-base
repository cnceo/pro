.box_table {
  width: 100%;
  overflow: hidden;
}

.list {
  float: left;
  width: 25%;
  height: 439px;
  line-height: 38px;
  box-sizing: border-box;
  > li:first-child {
    height: 38px;
  }
  > li:first-child {
    border: 1px solid $border_color;
    background-color: #eee;
    color: #999;
  }
  > li:nth-child(2) {
    color: rgb(64, 64, 64);
    font-size: 12px;
  }
}

.list2 {
  border-left: 1px solid $border_color;
  border-right: 1px solid $border_color;
  border-bottom: 1px solid $border_color;
  height: 400px;
}

.list:nth-child(2) {
  > li {
    border-left: none;
    border-right: none;
  }
  .list2 {
    border-left: none;
    border-right: none;
  }
}

.list:nth-child(3) {
  > li {
    border-right: none;
  }
  .list2 {
    border-right: none;
  }
}

.cla {
  height: 39px;
  display: inline-block;
  width: 70px;
  border-right: 1px solid $border_color;
  cursor: pointer;
  .box_bg {
    display: block;
    height: 35px;
    line-height: 35px;
    border-radius: 20px;
    color: #000;
    font-size: 14px;
    font-weight: 700;
    margin: 0 auto;
    margin-top: 2px;
  }
}

.ball {
  // color: #fff !important;
  // background: -prefix-linear-gradient(left top, #2698e2, white);
  // background: linear-gradient(to bottom right, #2698e2, white);
  // background-image: url("../../../assets/img/blue_box.png");
  // background-repeat: no-repeat;
  // background-position-x: 25px;
  // background-position-y: -1px;
  width: 30px;
  height: 30px;
  line-height: 30px;
  margin: 0 auto;
  margin-top: 5px;
  border-radius: 20px;
  background: $ball;
  display: block;
  color: #fff;
  font-weight: bold;
  font-size: 16px;
}

.cla2 {
  height: 39px;
  width: 60px;
  display: inline-block;
  font-weight: 700;
  margin: 0 auto;
  border-right: 1px solid $border_color;
  cursor: pointer;
}

.cla3 {
  height: 39px;
  width: 88px;
  display: inline-block;
  font-weight: 700;
  margin: 0 auto;
  input {
    width: 36px;
    border: 1px solid $border_color;
    border-radius: 3px;
    height: 20px;
    margin-top: 10px;
  }
}

.lis_2 {
  border-bottom: 1px solid $border_color;
}

.main {
  background-color: $bg_select;
}

.list:nth-child(1) .lis_2:last-child,
.list:nth-child(2) .lis_2:last-child {
